{
  "name": "electron-app",
  "version": "1.0.0",
  "description": "a electron app",
  "main": "main.js",
  "scripts": {
    "start": "concurrently \"npm run build:dev\" \"npm run start:dev\"",
    "start:dev": "cross-env ELECTRON_ENV=development DEBUG=electron WEBPACK_PORT=9005 electron .",
    "build:dev": "cross-env BABEL_ENV=development webpack server --hot --port 9005 --env env=development",
    "build:prod": "cross-env BABEL_ENV=production webpack --progress --env env=production",
    "icons": "electron-icon-builder --input=./src/icon.png --output=.",
    "pack:dev": "electron-builder --dir --config electron-builder.js",
    "pack:prod": "electron-builder --config electron-builder.js",
    "prettier": "prettier src/**/*.ts src/**/*.{ts,tsx} --write",
    "test": "jest .",
    "upgradePackages": "yarn upgrade --latest"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "autoUpdater": {
    "enable": false,
    "feedUrl": "http://127.0.0.1:9005/release"
  },
  "config": {
    "commitizen": {
      "path": "cz-emoji"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn prettier && yarn test"
    }
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/src/"
    ],
    "preset": "ts-jest"
  },
  "devDependencies": {
    "@ant-design/icons": "^4.7.0",
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@dr.pogodin/babel-plugin-react-css-modules": "^6.5.4",
    "@loadable/component": "^5.15.2",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.4",
    "@types/color": "^3.0.2",
    "@types/debug": "^4.1.4",
    "@types/jest": "^27.4.0",
    "@types/loadable__component": "^5.13.4",
    "@types/lodash": "^4.14.178",
    "@types/node": "^17.0.8",
    "@types/react": "^17.0.1",
    "@types/react-css-modules": "^4.6.2",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.3.2",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "antd": "^4.18.3",
    "antd-dayjs-webpack-plugin": "^1.0.6",
    "babel-jest": "^27.4.6",
    "babel-loader": "^8.0.6",
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.5.1",
    "css-minimizer-webpack-plugin": "^3.3.1",
    "cz-emoji": "^1.3.1",
    "dayjs": "^1.10.7",
    "electron": "^16.0.7",
    "electron-builder": "^22.9.1",
    "electron-icon-builder": "^2.0.1",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-webpack-plugin": "^3.1.1",
    "husky": "^7.0.4",
    "jest": "^27.4.7",
    "less": "^4.1.2",
    "less-loader": "^10.2.0",
    "lodash": "^4.17.21",
    "mini-css-extract-plugin": "^2.4.6",
    "mobx": "5",
    "mobx-react": "6",
    "postcss-less": "^6.0.0",
    "prettier": "^2.5.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-refresh": "^0.11.0",
    "react-router-dom": "5",
    "style-loader": "^3.3.1",
    "ts-jest": "^27.1.2",
    "typescript": "^4.1.3",
    "webpack": "^5.66.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.7.3"
  },
  "dependencies": {
    "color": "^4.2.0",
    "datauri": "^4.1.0",
    "electron-updater": "^4.3.5",
    "tinify": "^1.6.1"
  }
}
